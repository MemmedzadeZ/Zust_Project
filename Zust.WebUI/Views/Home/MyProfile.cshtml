@model PostAndProfileListViewModel
<style>
    .profile-cover-image {
        position: relative; 
    }

    #uploadButton {
        position: absolute;
        bottom: 10px; 
        right: 10px; 
       background-color: rgba(0, 0, 0, 0.5);  
        color: white; 
        border: none; 
        padding: 10px;
        border-radius: 5px; 
        cursor: pointer; 
    }

        #uploadButton:hover {
            background-color: rgba(0, 0, 0, 0.8); 
        }


    textarea {
        margin-bottom: 10px;
    }

    .btn-primary {
        background-color: #3644D9;  
        color: white; 
        border: none; 
        border-radius: 5px; 
        padding: 10px 15px; 
        font-size: 16px;
        cursor: pointer;
        transition: background-color 0.3s;
    }

        .btn-primary:hover {
            background-color: #0056b3;  
        }


    #imagePreview, #videoPreview {
        width: 100%; 
        max-width: 600px;
        height: auto; 
        margin-top: 10px; 
    }

    .post-body p {
        max-width: 100%; 
        word-wrap: break-word; 
    }


</style>


<div class="content-page-box-area">
  <div class="container my-profile-inner-box">
    <div class="row">
        <div class="col-12 profile-cover-image">
            <a href="#">
                <img src="@(!string.IsNullOrEmpty(ViewBag.User.BackgroundImage) ? Url.Content(ViewBag.User.BackgroundImage) : Url.Content("~/assets/images/my-profile-bg.jpg"))"
                     class="img-fluid rounded-top"
                     alt="Cover Image" style="height: 250px; object-fit: cover;">
            </a>
            <div class="position-relative">
                <form id="imageUploadForm" asp-action="ChangeCoverImage" asp-controller="Home" enctype="multipart/form-data" class="position-absolute top-0 end-0 m-3">
                    <input type="file" id="imageUpload" name="formFile" style="display: none;" />
                    <button type="button" class="btn btn-secondary" onclick="triggerFileUpload()">Edit Cover</button>
                </form>
            </div>
        </div>
    </div>

    <div class="row profile-info-box mt-3 bg-light p-3 rounded shadow-sm">
        <div class="col-md-4 text-center">
            <div class="info-image">
                <a href="#">
                    <img src="@(!string.IsNullOrEmpty(ViewBag.User.Image) ? Url.Content(ViewBag.User.Image) : Url.Content("~/images/user/user.png"))"
                         class="img-fluid rounded-circle border border-secondary"
                         style="width: 150px; height: 150px;"
                         alt="Profile Image">
                </a>
                <div class="icon position-absolute bottom-0 end-0">
                    <form id="imageUploadForm" asp-action="ChangeImageUrl" asp-controller="Home" enctype="multipart/form-data">
                        <input type="file" id="imageUpload" name="formFile" style="display: none;" />
                        <button type="button" class="btn btn-outline-secondary" onclick="triggerFileUpload()">
                            <i class="flaticon-photo-camera"></i>
                        </button>
                    </form>
                </div>
            </div>
        </div>
        <div class="col-md-8 d-flex align-items-center">
            <div class="info-text ms-3">
                <h3><a href="#" class="text-dark">@Model.ProfileView.Firstname @Model.ProfileView.Lastname</a></h3>
                <span><a href="mailto:@(Model.ProfileView.Email)" class="text-muted">@Model.ProfileView.Email</a></span>
                <ul class="statistics d-flex mt-3">
                    <li class="me-3">
                        <span class="item-number">59862</span>
                        <span class="item-text">Likes</span>
                    </li>
                    <li>
                        <span class="item-number">8591</span>
                        <span class="item-text">Friends</span>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="profile-list-tabs mt-3">
        <ul class="nav nav-tabs bg-light border-0" id="myTab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active text-primary" id="timeline-tab" data-bs-toggle="tab" href="#timeline" role="tab" aria-controls="timeline">Timeline</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-muted" id="about-tab" data-bs-toggle="tab" href="#about" role="tab" aria-controls="about">About</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-muted" id="friends-tab" data-bs-toggle="tab" href="#friends" role="tab" aria-controls="friends">Friends</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-muted" id="photos-tab" data-bs-toggle="tab" href="#photos" role="tab" aria-controls="photos">Photos</a>
            </li>
        </ul>
    </div>

    <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="timeline" role="tabpanel">
            <div class="news-feed-area mt-3">
                <h3 class="news-feed-title text-primary">Create New Post</h3>
                <form id="uploadForm" enctype="multipart/form-data">
                    <div class="form-group">
                        <textarea id="message" class="form-control" placeholder="Write something here..." style="height: 100px;"></textarea>
                    </div>
                    <div class="d-flex justify-content-between mt-2">
                        <button type="button" class="btn btn-outline-secondary" onclick="document.getElementById('imageUpload').click()"><i class="flaticon-gallery"></i> Photo</button>
                        <button type="button" class="btn btn-outline-secondary" onclick="document.getElementById('videoUpload').click()"><i class="flaticon-video"></i> Video</button>
                        <button type="submit" class="btn btn-success">Post</button>
                    </div>
                </form>

                <div id="posts" class="mt-3">
                    @foreach (var item in Model.MyPostList)
                    {
                        @await Html.PartialAsync("_PostPartial", new PostPartialViewModel { Post = item });
                    }
                </div>

                <div class="load-more-posts-btn mt-3">
                    <a href="#" class="btn btn-link text-primary"><i class="flaticon-loading"></i> Load More Posts</a>
                </div>
            </div>
        </div>
    </div>
</div>










